# Environment configuration for the Mesh-terious Warehouse stack

# RabbitMQ settings
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest

# MinIO / Iceberg
MINIO_ENDPOINT=http://minio:9000
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
ICEBERG_CATALOG=minio
ICEBERG_REST_URI=http://iceberg-rest:8181
ICEBERG_WAREHOUSE=s3://warehouse

# OpenMetadata
OPENMETADATA_HOSTPORT=http://openmetadata:8585/api

# Airflow
AIRFLOW_UID=50000
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin
_PIP_ADDITIONAL_REQUIREMENTS=pika pyarrow pyiceberg openmetadata-ingestion pydantic duckdb dbt-core dbt-duckdb

# Superset
SUPERSET_SECRET_KEY=CHANGE_ME
SUPERSET_SQLALCHEMY_DATABASE_URI=duckdb:///data/warehouse.duckdb

# Default admin credentials for Superset
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=admin
SUPERSET_ADMIN_FIRSTNAME=Admin
SUPERSET_ADMIN_LASTNAME=User
SUPERSET_ADMIN_EMAIL=admin@example.com

# DuckDB
DUCKDB_PATH=/data/warehouse.duckdb
